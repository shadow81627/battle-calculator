{
  "name": "battle-calculator",
  "private": true,
  "packageManager": "yarn@4.1.0",
  "description": "Tabletop battle simulation",
  "scripts": {
    "build": "nuxi build",
    "dev": "nuxi dev",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
    "start": "node .output/server/index.mjs",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "nuxi prepare",
    "generate": "nuxi generate",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal-2023 public/logo.svg",
    "start:generate": "npx serve .output/public"
  },
  "dependencies": {
    "@vueuse/components": "10.2.1",
    "@vueuse/core": "10.2.1",
    "cheerio": "1.0.0-rc.12",
    "fast-xml-parser": "4.2.4",
    "jsdom": "24.0.0",
    "lodash-es": "4.17.21",
    "slugify": "1.6.6",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.39.5",
    "@iconify-json/carbon": "1.1.18",
    "@iconify-json/twemoji": "1.1.11",
    "@nuxt/content": "2.9.0",
    "@nuxt/devtools": "0.6.2",
    "@nuxt/eslint-config": "0.2.0",
    "@nuxtjs/color-mode": "3.3.0",
    "@types/jsdom": "21.1.6",
    "@types/lodash-es": "4.17.7",
    "@types/uuid": "^9",
    "@unocss/eslint-config": "0.53.1",
    "@unocss/nuxt": "0.58.4",
    "@vite-pwa/assets-generator": "0.2.4",
    "@vite-pwa/nuxt": "0.5.0",
    "@vueuse/nuxt": "10.2.0",
    "consola": "3.1.0",
    "eslint": "8.43.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "nuxt": "3.9.3",
    "nuxt-icon": "0.6.8",
    "prettier": "3.2.5",
    "typescript": "5.1.3"
  }
}
